<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="AUTO NO :"
        android:id="@+id/autotextview1"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignParentTop="true"
        android:layout_marginTop="44dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/autono"
        android:textColor="@color/colorPrimaryDark"
        android:layout_alignBottom="@+id/autotextview1"
        android:layout_alignLeft="@+id/callbuttton"
        android:layout_alignStart="@+id/callbuttton"
        android:layout_alignRight="@+id/callbuttton"
        android:layout_alignEnd="@+id/callbuttton" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NAME   :"
        android:id="@+id/name1"
        android:textColor="@color/colorPrimaryDark"
        android:layout_below="@+id/autotextview1"

        android:layout_marginTop="48dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/avl_name"
        android:layout_alignTop="@+id/mobile_no1"
        android:layout_centerHorizontal="true"></TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MOBILE NO :"
        android:textColor="@color/colorPrimaryDark"
        android:id="@+id/mobile_no1"
        android:layout_above="@+id/callbuttton"
        android:layout_alignRight="@+id/autotextview1"
        android:layout_alignEnd="@+id/autotextview1"
        android:layout_marginBottom="74dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:textColor="@color/colorPrimaryDark"
        android:id="@+id/mobile_no"
        android:layout_alignTop="@+id/name1"
        android:layout_alignRight="@+id/autono"
        android:layout_alignEnd="@+id/autono"></TextView>


    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/callbuttton"
        android:src="@drawable/ic_call_black_24dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="127dp" />




    </RelativeLayout>